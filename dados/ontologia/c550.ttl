###  http://jcr.di.uminho.pt/m51-clav#c550.10.305
:c550.10.305 rdf:type owl:NamedIndividual ,
	:Classe_N3;
	:codigo "550.10.305";
	:titulo "Processamento de pedidos de asilo";
	:temPai :c550.10 ;
	:processoTipo "PE" ;
	:processoTransversal "S" ;
	:temParticipanteIniciador :org_PSP ;
	:temParticipanteIniciador :org_GNR ;
