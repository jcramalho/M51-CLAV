	:titulo "Atribuição de alimentos a filhos maiores ou emancipados";
	:temPai :c600.30 ;
	:processoTipo "PE" ;
	:processoTransversal "S" ;
	:temParticipanteApreciador :org_MP ;
	:temDono :org_Trb ;
	:temNotaAplicacao :na_c600.30.283_1 ;
	:temNotaExclusao :ne_c600.30.283_1 ;
