###  http://jcr.di.uminho.pt/m51-clav#c600.40.001
:c600.40.001 rdf:type owl:NamedIndividual ,
	:Classe_N3;
	:codigo "600.40.001";
	:titulo "Acompanhamento da execução de pena e medida privativa da liberdade";
	:temPai :c600.40 ;
	:processoTipo "PE" ;
	:processoTransversal "S" ;
	:temDono :org_TEP ;
	:temNotaAplicacao :na_c600.40.001_1 ;
	:temNotaAplicacao :na_c600.40.001_2 ;
	:temNotaAplicacao :na_c600.40.001_3 ;
