###  http://jcr.di.uminho.pt/m51-clav#c600.40.400
:c600.40.400 rdf:type owl:NamedIndividual ,
	:Classe_N3;
	:codigo "600.40.400";
	:titulo "Procedimento extrajudicial pré-executivo";
	:temPai :c600.40 ;
