###  http://jcr.di.uminho.pt/m51-clav#c600.30.101
:c600.30.101 rdf:type owl:NamedIndividual ,
	:Classe_N3;
	:codigo "600.30.101";
	:titulo "Internamento compulsivo";
	:temPai :c600.30 ;
	:processoTipo "PE" ;
	:processoTransversal "S" ;
	:temParticipanteAssessor :org_DGRSP ;
	:temParticipanteAssessor :org_OPC ;
