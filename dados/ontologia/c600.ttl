###  http://jcr.di.uminho.pt/m51-clav#c600.30.151
:c600.30.151 rdf:type owl:NamedIndividual ,
	:Classe_N3;
	:codigo "600.30.151";
	:titulo "Mandado de detenção europeu";
	:temPai :c600.30 ;
	:processoTipo "PE" ;
	:processoTransversal "S" ;
	:temParticipanteAssessor :org_PJ ;
	:temDono :org_MP ;
	:temNotaAplicacao :na_c600.30.151_1 ;
